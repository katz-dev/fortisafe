{
    "manifest_version": 3,
    "name": "FortiSafe Extension",
    "version": "1.0.0",
    "description": "A browser extension built with Next.js",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {}
}